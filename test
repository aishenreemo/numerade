<script>
$('#modal-login-form').on('submit', function(e){
  e.preventDefault();
  $('.login-email-error, .login-password-error').text('');
  let loginEmail = $('#modal-login-email').val();
  let loginPassword = $('#modal-password-email').val();
  let cleanLoginData = true;
  if(!loginEmail){
    $('.login-email-error').text('Please enter your email.').css('color', 'red');
    cleanLoginData = false;
  }
  if(!loginPassword){
    $('.login-password-error').text('Please enter your password.').css('color', 'red');
    cleanLoginData = false;
  }
  if(!cleanLoginData){
    return;
  }
  $('.login-error').text("")
  addSpinner();
  $.ajax({
    url: '/login/',
    method: 'POST', 
    data: $(this).serialize(),
    success: function(response){
      window.location.reload();
      removeSpinner();
    },
    error: function(response){
      console.log(response);
      $('.login-error').text("An error occured while loggin you in.").css('color', 'red');
      removeSpinner();
    }
  })
})
</script>





