<script>
  var buttonText = { 
    // 'annual': "",
    // 'monthly': ""
    'annual': "Continue",
    'monthly': "Continue"
  };

  $('.subscribe-modal-option').on('click', function(e){
    let prices = {
      'annual': '$6.99',
      'quarterly': '$8.99',
      'monthly': '$9.99',
    };
    let plan = $(this).attr('id');
    $('.start-trial-button').text(buttonText[plan]);
    $('.subscribe-modal-option').removeClass('selected-modal-option')
    $(this).addClass('selected-modal-option');
    $('.faux-radio-button > div').removeClass('selected-faux-radio-button')
    $(this).find('.faux-radio-button > div').addClass('selected-faux-radio-button');

    $('.subscription-modal-plan-dropdown > option').removeAttr('selected');
    $(`.subscription-modal-plan-dropdown > option[value='${plan}']`).attr('selected', 'true');
    $('span.billed-today').text(prices[plan]);
  });

$('.subscribe-modal-options-a-c').on('click', function(e){
  let prices = {
    'annual': '$6.99',
    'monthly': '$9.99',
  }
  let plan = $(this).attr('id');
  planType = plan
  //$('.start-trial-button').text(buttonText[plan]);
  if (typeof planDict != "undefined") {
    planId = planDict[plan];
  }
  
  $('.subscribe-modal-options-a-c').removeClass('selected-modal-option-a')
  $(this).addClass('selected-modal-option-a');
  $('.subscription-modal-plan-dropdown > option').removeAttr('selected');
  $(`.subscription-modal-plan-dropdown > option[value='${plan}']`).attr('selected', 'true');

  // Update the hidden input value 
  $('#plan-select-option').val(plan);

  // Update the third modal selected plan that is visible to users 
  $('.sub-modal-main-selected').hide();
  $(`#subscription-modal-selected-${plan}`).show();

  $(`.sub-modal-main-terms`).hide();
  $(`.sub-modal-main-terms__${plan}`).show();

  $('span.billed-today').text(prices[plan]);
  $('#annual_mobile').addClass('selected-modal-option-a');
  $('#monthly_mobile').addClass('selected-modal-option-a');
  $('#annual_mobile').hide();
  $('#monthly_mobile').hide();
  $('#'+plan+'_mobile').show();
  $('#'+plan).show();
});

$('#plan-select-option').on('change', function(e){
  e.preventDefault();
  let plan = $(this).val();
  if (typeof planDict != "undefined") {
    planId = planDict[plan];
  }
  $('.start-trial-button').text(buttonText[plan]);
})

$('.subscribe-modal-b-button').on('click', function(e){
  let prices = {
    'annual': '$6.99',
    'monthly': '$9.99',
  }
  let plan = $(this).attr('id');
  $('.subscribe-modal-options-a-c').removeClass('selected-modal-option-a')
  $(this).addClass('selected-modal-option-a');
  $('.subscription-modal-plan-dropdown > option').removeAttr('selected');
  $(`.subscription-modal-plan-dropdown > option[value='${plan}']`).attr('selected', 'true');
  $('span.billed-today').text(prices[plan]);
});



// Code for applying promo code
$('.apply-promo-code').on('click', function(e){
  e.preventDefault();
  let code = $('input[name="promo_code"]').val();
  if(code){
    $.ajax({
      url: '/payments/promo-code/',
      method: 'POST',
      data: {
        'csrfmiddlewaretoken': "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH",
        'code': code,
      },
      success: function(response){
        $('.promo-code-message').text("Promo code is valid. Updating pricing...");
        window.location.href = window.location.href + "?show-modal=true";
      }, 
      error: function(response){
        $('.promo-code-message').text("Promo code is not valid.")
      }
    })
  }
})
</script>




