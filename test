    $('.navbar-b').on('click', '.hamburger-menu', function(){
      const overlayNavbar = $('.mobile-navbar-overlay');
      const that = $('.hamburger-menu-inner');
      if(overlayNavbar.hasClass('opened')){
          overlayNavbar.removeClass('opened');
          that.removeClass('x-btn');
      }else{
          overlayNavbar.addClass('opened');
          that.addClass('x-btn');
      }
  })
