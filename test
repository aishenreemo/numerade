<script src="/static/tiktok.e63fb36224a6.js" type="text/javascript"></script>
<script>
function cleanSignupForm(){
  $('.signup-fullname-error, .signup-email-error, .signup-password-error, .signup-parent-error').text('').css('color', 'red');
  let full_name = $('#centralSignupForm input[name="full_name"]').val();
  let email = $('#centralSignupForm input[name="email"]').val();
  let password = $('#centralSignupForm input[name="password"]').val();
  let student_parent = $('#centralSignupForm input[name="student_parent"]:checked').val()
  let clean = true;
  if(full_name.split(" ").length < 2){
    $('.signup-fullname-error').text("Please enter your full name");
    clean = false;
  }
  if(!email){
    $('.signup-email-error').text("Please enter your email");
    clean = false;
  }
  if(!password){
    $('.signup-password-error').text("Please enter your password");
    clean = false;
  }
  if(!student_parent){
    $('.signup-parent-error').text('Please select student or parent');
    clean = false;
  }

  return clean;
}
$('#centralSignupForm').on('submit', function(e){
  e.preventDefault();
  const cleanData = cleanSignupForm();
  if(!cleanData){
    return;
  }
  addSpinner();

  let googleAdCohort = "";
  let signupUrl = googleAdCohort === "student" || googleAdCohort === "parent" ? `/signup/?type=student&campaign=${googleAdCohort}` : '/signup/?type=student';

  let formData = $(this).serialize();
  let paymentDirect = "";

  formData += `&formLoc=${window.location.pathname}`;

  $.ajax({
    url: signupUrl,
    method: 'POST',
    data: formData,
    success: function(response) {
      removeSpinner();
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        'event': 'account_creation',
        'account_type': 'Email'
      });

      dataLayer.push({'event': 'async-user-signup'});
      emitTikTok('CompleteRegistration')

      if ("True" === "True") {
        $('#subscription-modal-response-container').html(response)
        $('#register-modal').modal('hide')
        $('#qd-login-modal').modal('hide')
        $('#subscribe-modal').modal('show')
        
        $.ajax({
          url: '/questions/userevent/',
          method: 'POST',
          data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "PMV"}
          }).done(function(response) {
        });
      }else{
        if(paymentDirect === "True"){
          window.location.href = '/payments/';
        } else {
          window.location.href = window.location.pathname + "?showSuccessModal=true";
          removeSpinner();
        }
      }
    },
    error: function(response){
      let sign_up_error = "Please try again later."

      const messages = response.responseJSON.message
        for (let i = 0; i < messages.length; i++) {
          if (Object.keys(messages[i]) == 'email') {
            $('.signup-email-error').text(Object.values(messages[i]));
          }
        }

      if (typeof response.responseJSON !== 'undefined' && response.responseJSON.message == 'duplicate') {
        sign_up_error = "It appears an account with this email already exists. Please log in."
      }
      $('#sign-up-error').text(sign_up_error);
      console.log(response);
      removeSpinner();
    }
  })
})
</script>

<script>
  // UserTracker 
  $('#register-modal').on('shown.bs.modal', function(){
    $.ajax({
      url: '/user-event-tracker/', 
      method: 'POST', 
      data: {
        csrfmiddlewaretoken: 'o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf',
        campaign: 'student_parent_test_20220110',
        cohort: 'student_parent_form',
        action: 'VIE'
      },
      success: function(res){
        console.log('Event tracking 200')
      }
    })
  });

  $("#parent-student-check").click(function(){
    $('.signup-parent-error').css('color', 'red');
    $('.signup-parent-error').text('Please select student or parent');
  });
  
  $("#student, #parent").click(function(){
    $("#parent-student-check").css("z-index", 8);
    $('.signup-parent-error').text('');
    let temp = $('input[name="student_parent"]:checked').val();
    $.ajax({
      url: '/session-handler/', 
      method: 'POST', 
      data: {
        csrfmiddlewaretoken: 'o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf',
        key: 'student_parent',
        value: temp
      },
      success: function(res){
        console.log('Set student_parent')
      }
    })
  });

  $('#register-modal').on('hidden.bs.modal', function (e) {
      if (open_register_modal_time > new Date()) {
        $.ajax({
          url: '/questions/userevent/',
          method: 'POST',
          data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "BRM"}
          }).done(function(response) {
        });
      }
  });

  var open_register_modal_time;
  $('#register-modal').on('shown.bs.modal', function() {
    open_register_modal_time = new Date();
    open_register_modal_time.setSeconds(open_register_modal_time.getSeconds() + 4);
    
    $.ajax({
      url: '/user-event-tracker/',
      method: 'POST',
      data: {
        csrfmiddlewaretoken: 'o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf',
        campaign: 'student_parent_test_20220110',
        cohort: 'student_parent_form',
        action: 'VIE',
      },
      success: function(res) {
        console.log('Event tracking 200');
      },
    });
