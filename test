<script src="/static/videojs-event-tracking/videojs-event-tracking.b30b04635c81.js"></script>
<script>
var playerInstance;
// Store video url so we can re use if the video doesn't load correctly the first time.
playerInstance = videojs("my-video");

var currentAnswerId = "13323";
var currentRecommendationId = "";
var rejectedRecommendations = [];

if("ask" == "practical"){
  videoUrl = "7e0fc633-fbed-4ca3-a2be-f996db1c852b";
  bucketFolder = "encoded"
}
else{
  videoUrl = "7e0fc633-fbed-4ca3-a2be-f996db1c852b";
  bucketFolder = "ask_video";
}

playerInstance.eventTracking(
  {
    "percentile": {
      "highFloat": 0.75,
      "lowFloat": 0.6
    }
  }
);

playerInstance.ready(function() {
  // analytics.track('Video Loaded', {...questionProps, ...nonInteractive});
  player = this;
  player.on('play', function() {
    $.ajax({
        url: '/questions/userevent/',
        method: 'POST',
        data:{csrfmiddlewaretoken: "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH", event: "PBC"}
        }).done(function(response) {
      });
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        'event': 'video_play_click',
        'video_title': 'i-a-tiger-leaps-horizontally-from-a-75-m-high-rock-with-a-speed-of-30-ms-how-far-from-the-base-of-the-rock-will-she-land-2'
      });
    // analytics.track('Video Played', questionProps);
  });

  // TikTok link after answer video plays
  if($('.tiktok-modal-container').length) {
    $('#my-video').append($('.tiktok-modal-container'))
    playerInstance.on('ended', function() {
      $('.tiktok-modal-container').css('display', 'flex');
      $('#my-video').css('min-height', '1000px')

      $('#replayVideo').click(function() {
        var tiktok_video_container = $('.tiktok-modal__video');
        var tiktok_video_content = tiktok_video_container.html();
        tiktok_video_container.html(tiktok_video_content);
        $('.tiktok-modal-container').css('display', 'none');
        $('#my-video').css('min-height', '39rem');
        videojs('my-video').play();
      })
    })
  }

  // Quiz after answer video plays
  
});

// AJAX call to retrieve quiz data after video ends and quiz gets created
function getQuizQuestions(quiz_id){
  $.ajax({
    url: '/questions/api/q_quiz_questions/'+quiz_id,
    method: 'GET',
    success: function(res){
      $('.video-quiz-container').html(res)
      MathJax.Hub.Typeset();
      removeSpinner();
    }
  });
}

playerInstance.on('seeking', function(){
  $('.nextVideo').css('display', 'none');
});


  playerInstance.on('tracking:sign-up-wall', (e, data) => {
    playerInstance.pause();
    $('.ask-signupwall-overlay').show();
    $('#register-modal').modal('show');
    $('#my-video').remove();
  });




playerInstance.on('error', function(){
  playerInstance.src({
    type: "video/mp4",
    src: `https://cdn.numerade.com/${bucketFolder}/${videoUrl}.mp4`
  });
});

