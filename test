<script>
  function renderButton() {
    appStart();

    let width = $('.central-signup-box__form-btn').outerWidth();
    if(width == undefined){
      width = "300px";
    }
    gapi.signin2.render('g-signin2', {
      'scope': 'profile email',
      'width': width,
      'height': 40,
      'longtitle': true,
      'margin': '0 auto',
    });
    gapi.signin2.render('qd-g-signin2', {
      'scope': 'profile email',
      'width': width,
      'height': 40,
      'longtitle': true,
      'margin': '0 auto',
    });
  }
</script>

<script src="https://apis.google.com/js/platform.js?onload=renderButton"></script>
<script>
  var auth2; // The Sign-In object.
  var googleUser; // The current user.

  var signinChanged = function (val) {
    if(val == true){
      googleUser = auth2.currentUser.get();
      profile = googleUser.getBasicProfile();
      $.ajax({
        url: '/oauth/google/',
        method: 'POST',
        data: {
          csrfmiddlewaretoken: "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH",
          name: profile.getName(),
          email: profile.getEmail(),
          googleId: googleUser.getAuthResponse().id_token,
          renderedResponseNeeded: true,
        },
        success: function(res){
          if (res.status == "Success") {
            
              window.location.reload();
            
          } else {
          
            
              $('#subscription-modal-response-container').html(res);
              $('#register-modal').modal('hide');
              $('#qd-login-modal').modal('hide');
              $('#subscribe-modal').modal('show');
            
          
          }
        },
        error: function(res){
          console.log(res.message);
        }
      });
    }
  };

  var refreshValues = function() {
    if (auth2){
      googleUser = auth2.currentUser.get();
    }
  }

  /**
   * Initializes Signin v2 and sets up listeners.
   */
  var initSigninV2 = function() {
    auth2 = gapi.auth2.init({
      client_id: '406892979854-gq7pb0ka6lacplgj779ohoqb0ohmfmtf.apps.googleusercontent.com',
      scope: 'profile email'
    });

    // Set Handler for UserEvent logging
    setGoogleClickEvents(auth2);

    // Listen for sign-in state changes.
    auth2.isSignedIn.listen(signinChanged);

    // Sign in the user if they are currently signed in.
    if (auth2.isSignedIn.get() == true) {
      auth2.signIn();
    }

    // Start with the current live values.
    refreshValues();
  };

  var appStart = function() {
    gapi.load('auth2', initSigninV2);
  };
  
  function setGoogleClickEvents(auth2) {
    // get all versions of the Google sign in buttuon
    let googleButton = document.getElementById('g-signin2');    
    let googleButton2 = document.getElementById('qd-g-signin2');

    auth2.attachClickHandler(googleButton, {}, function(user) {
      // Log UserEvent record for SSO Google Click
      googleUserEventPost();
    }, function (error) {
      // Also log UserEvent record for any error, this includes a user closing the window
      googleUserEventPost();
    });

    // Repeat for qd-g-signin2 button
    auth2.attachClickHandler(googleButton2, {}, function(user) {
      googleUserEventPost();
    }, function (error) {
      googleUserEventPost();
    });
  };

  function googleUserEventPost() {
    $.ajax({
      url: '/questions/userevent/',
      method: 'POST',
      data:{csrfmiddlewaretoken: "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH", event: "SGC"}
      }).done(function(response) {
      });
  };
</script>

<!-- Facebook SSO  -->
<script>
  $(".fb-login-button").data('width', '300').css({'border-radius': '0.8rem', 'font-size': '1.6rem'})

  function loginFacebookUser(response){
    // Log UserEvent record for SSO Facebook Click
    $.ajax({
      url: '/questions/userevent/',
      method: 'POST',
      data:{csrfmiddlewaretoken: "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH", event: "SFC"}
      }).done(function(response) {
      });
    FB.api('/me', {locale: 'en_US', fields: 'id,first_name,last_name,email,link,gender,locale,picture'}, function(res){
      let token = {
        'csrfmiddlewaretoken': 'a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH'
      }
      addSpinner();
      $.ajax({
        url: '/oauth/facebook/',
        method: 'POST',
        data: {...token, ...response.authResponse, ...res, renderedResponseNeeded: true},
        success: function(response){
          if (response.status == "Success") {
            
              window.location.reload();
            
          } else {
          removeSpinner();
          
            
              $('#subscription-modal-response-container').html(response);
              $('#register-modal').modal('hide');
              $('#qd-login-modal').modal('hide');
              $('#subscribe-modal').modal('show');
            
          
          }
        },
        error: function(res){
          removeSpinner();
          FB.login(function(response) {}, { auth_type: 'reauthenticate' });
          if(res.responseJSON.message == 'No Email'){
            errMessageHtml = `<p style="margin-top: 1rem;" class="flash-message error">Your email is required to login/sign up with Facebook. Please adjust your Facebook permissions to ensure your email is included.</p>`
            $(errMessageHtml).insertAfter('.fb-login-button')
          }
        }
      })
    });
  }
  $.ajaxSetup({ cache: true });
  $.getScript('https://connect.facebook.net/en_US/sdk.js', function(){
  FB.init({
    appId: '402458547807517',
    version: 'v2.7'
  });
  FB.getLoginStatus(function(response){
    if(response.status == 'connected'){
      loginFacebookUser(response);
    } else {
      $('#loginbutton,#feedbutton').removeAttr('disabled');
      FB.Event.subscribe('auth.login', function(response){
        loginFacebookUser(response);
      });
      FB.Event.subscribe('auth.logout', function(response){
        console.log(response);
      });
    }
  });

  });
</script>
<script type="text/javascript" src="https://appleid.cdn-apple.com/appleauth/static/jsapi/appleid/1/en_US/appleid.auth.js"></script>
