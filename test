    /* start bouncing_question_immediately and bouncing_paywall_prompt */
    var bouncing_question_immediately_time = new Date();
    bouncing_question_immediately_time.setSeconds(bouncing_question_immediately_time.getSeconds() + 4);

    if (typeof bouncing_paywall_prompt_enable !== 'undefined') { 
        if (bouncing_paywall_prompt_enable) {
            var bouncing_paywall_prompt_time = new Date();
            bouncing_paywall_prompt_time.setSeconds(bouncing_paywall_prompt_time.getSeconds() + 4);
        }
    }

    window.onunload = function bouncing_question_immediately(event) {
        var url = "/questions/userevent/";
        var data = new FormData();
        
        let csrfToken = 'a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH';

        data.append('csrfmiddlewaretoken', csrfToken);      
        data.append('event', 'BQI');

        if (bouncing_question_immediately_time > new Date()) {
            var status = navigator.sendBeacon(url, data);
        }

        if (typeof bouncing_paywall_prompt_time !== 'undefined') {
            if (bouncing_paywall_prompt_time > new Date()) {
                data.append('event', 'BPP');
                var status = navigator.sendBeacon(url, data);
            }
        }
    };
    /* end bouncing_question_immediately and bouncing_paywall_prompt */
