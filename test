// Code for applying promo code
$('.apply-promo-code').on('click', function(e){
  e.preventDefault();
  let code = $('input[name="promo_code"]').val();
  if(code){
    $.ajax({
      url: '/payments/promo-code/',
      method: 'POST',
      data: {
        'csrfmiddlewaretoken': "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf",
        'code': code,
      },
      success: function(response){
        $('.promo-code-message').text("Promo code is valid. Updating pricing...");
        window.location.href = window.location.href + "?show-modal=true";
      }, 
      error: function(response){
        $('.promo-code-message').text("Promo code is not valid.")
      }
    })
  }
})
</script>




