            if(email !== ''){
                $.ajax({
                url: '/send-email-app/',
                method: 'post',
                data: {
                    'email': email,
                    'csrfmiddlewaretoken': 'a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH'
                }
            }).done(function(response) {
                $('.banner-email').hide();
                $('.button-container').hide();
                $('.banner-success-box').css('display', 'inline-flex');
