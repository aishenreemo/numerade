<script>

$('#parentEmailForm').on('submit', function(e){
  e.preventDefault();
  
  addSpinner();

  let signupUrl = '/parent-help/'; // add endpoint 
  let formData = $(this).serialize();
  
  formData += `&formLoc=${window.location.pathname}`;

  $.ajax({
    url: signupUrl,
    method: 'POST',
    data: formData,
    success: function(response){
      removeSpinner();
      window.location.reload();
    },
    error: function(response){
      removeSpinner();
      window.location.reload();
      
    }
  })
})

$('#parent-pay-modal').on('hidden.bs.modal', function (e) {
  if (typeof parent_pay_refresh_page !== 'undefined') { 
    window.location.reload();
  }
});
</script>


