<script>
  
  
  function showNewRecommendedVideo(el){
    let that = $(el);
    let recommendationId = that.data('recommendation-id');
    let recommendationBucket = that.data('recommendation-bucket');
    let recommendationVideoUrl = that.data('recommendation-video');
    let newPoster = "";

    currentRecommendationId = recommendationId;
    if(recommendationBucket == "encoded"){
      newPoster=`https://cdn.numerade.com/previews/${recommendationVideoUrl}_large.jpg`;
    }else{
      newPoster=`https://cdn.numerade.com/ask_previews/${recommendationVideoUrl}_large.jpg`;
    }

    $('.vjs-poster').css('background-image', `url('${newPoster}')`);

    playerInstance.src({
      poster: newPoster,
      type: "video/mp4",
      src: `https://cdn.numerade.com/${recommendationBucket}/${recommendationVideoUrl}.mp4`
    });

    document.querySelector('#my-video').scrollIntoView({
      behavior: 'smooth'
    });

    $("#reject-recommendation").css('color', '#717171');
  }
  $('.change-recommended-video').on('click', function(e){
    e.preventDefault();
    showNewRecommendedVideo(this);

  });

  $('#result-replay').click(function(){
    $('.recommendation-results').hide();
    $('#my-video').show();
    playerInstance.play();
  });

  $('#result-yes').click(function(){
    addSpinner();
    $.ajax({
      url: `/ask/question/49658/accept/`,
      method: 'POST',
      data: {
        recommendation_id: currentRecommendationId,
        csrfmiddlewaretoken: 'a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH'
      },
      success: function(res){
        removeSpinner();
        $("#approve-recommendation").css('color', 'green');
        $("#reject-recommendation").css('color', '#717171');
      },
      error: function(err){
        removeSpinner();
        console.log(err);
      }
    })
  });

  $('#result-no').click(function(){
    $("#reject-recommendation").css('color', 'red');
    let recommendOptions = $('.change-recommended-video');
    // Add current recommendation to a list of rejected Ids
    rejectedRecommendations.push(currentRecommendationId);
    recommendOptions.each(function(idx){
      currentEl = $(this);
      recommendationId = currentEl.data('recommendation-id');
      if (!rejectedRecommendations.includes(recommendationId)){
        showNewRecommendedVideo(this);
        return false;
      }
    })
  });

  $('#recommendation-success').click(function(){
    // Reset page and refresh, or just refresh?
    window.location = "/ask/question/i-a-tiger-leaps-horizontally-from-a-75-m-high-rock-with-a-speed-of-30-ms-how-far-from-the-base-of-the-rock-will-she-land-2";
  })

  $("#recommendation-fail-button").click(function(){
    $('.recommendation-results').hide();
    $("#recommendation-fail").hide();
    $('#my-video').show();
  });
  // Playlist Code ===========================

  function display_playlist_message(bookmarked, created){
  let container = $('.flash-container');
  container.html('');
  if(created){
    let playlistCreatedMessage = `<div class="flash-row"><div class="flash-row-c"><p class="flash-message success">Playlist succesfully created</p></div></div>`;
    container.append(playlistCreatedMessage);
  }
  if(bookmarked){
    let videoBookmarkedMessage = `<div class="flash-row"><div class="flash-row-c"><p class="flash-message success">Video added to playlist</p></div></div>`;
    container.append(videoBookmarkedMessage);
  }
  }

  $('#add-playlist-form').submit(function(e) {
    e.preventDefault();
    addSpinner();
    $.ajax({
      url: '/playlists/add/',
      method: 'post',
      data: $('#add-playlist-form').serialize()
    }).done(function(response) {
      removeSpinner();
      $('#bookmark-modal').modal('toggle');
      $('.d-bookmark-solid').attr('class', 'd-icon d-bookmark-solid solution-question__bookmark--is-active');
      display_playlist_message(true, false);
    });
  });

$('#create-playlist-form').submit(function(e) {
  addSpinner();
  e.preventDefault();
  $.ajax({
    url: '/playlists/create/',
    method: 'post',
    data: $('#create-playlist-form').serialize()
  }).done(function(response) {
    display_playlist_message(true, true);
    removeSpinner();
    $('#bookmark-modal').modal('toggle');
    if($('.d-bookmark-solid').attr('class') == 'd-icon d-bookmark-solid solution-question__bookmark--is-active') {
      $('.d-bookmark-solid').attr('class', 'd-icon d-bookmark solution-question__bookmark');
    } else {
      $('.d-bookmark-solid').attr('class', 'd-icon d-bookmark-solid solution-question__bookmark--is-active');
    }
  });
});
// End Playlist Code =======================================

// Comment Code ============================================
$('#commentForm').submit(function(e) {
  e.preventDefault();
  $.ajax({
  url: '/ask/comment/',
  method: 'post',
  data: $('#commentForm').serialize()
  }).done(function(response) {
    let html = $('#comment-list-cont').html();
    let newCommentHtml = `<div class="qd-comment">
                          <div class="qd-comment__header">`
                if (response.img) {
                    newCommentHtml += `<img src="${response.img}">`
                } else {
                    newCommentHtml += `<div class="solution-card-body__educator-img-no-icon" >${response.initials}</div>`
                }
                newCommentHtml += `
        <p class="qd-comment__header-name">${response.user}</p>
        <p class="qd-comment__header-date">${response.date}</p>
        </div>
        <div class="qd-comment__body">
          <p>${response.text}</p>
        </div>
        </div>`;

                $('.qd-comment-container').append(newCommentHtml);
                $('#commentForm')[0].reset();
            });
        });

        $('#discussion-comment-input').on('change paste keyup', function () {
            if ($.trim(this.value).length) {
                $('button.discussion-submit').css('background-color', '#5B49E2').removeAttr('disabled');
            } else {
                $('button.discussion-submit').css('background-color', '#CBCBCB').attr('disabled', true);
            }
        });

        $('.cancel-discussion').on('click', function (e) {
            e.preventDefault();
            $('#discussion-comment-input').val('');
        });
        // End Comment Code =======================================
        // Share Code =============================================
        $('#shareBtn').click(function (e) {
            e.preventDefault();
            $.ajax({
                url: '/ask/ask-share/',
                method: 'post',
                data: $('#emailListForm').serialize()
            }).done(function (response) {
                $('[name="email"]').val("");
            });
            $('#share-modal').modal('toggle');
        });

        $('[data-toggle="tooltip"]').tooltip();
        $('.copy-share-link').click(function () {
            var copyText = document.getElementById('id-copy-share-link');
            copyText.select();
            copyText.setSelectionRange(0, 99999); /*For mobile devices*/
            document.execCommand("copy");
            $('#id-copy-share-link').css('border-color', 'green');
            $('.copy-share-link').attr('title', 'Copied!').data('original-title', 'Copied!');
        })
        $('.radio-button-flag').click(function () {
            $('.radio-button-flag-label').removeClass('checked');
            $(this).parent('.radio-button-flag-label').addClass('checked');
        });
        // End Share Code =======================================

        

        
            $(".purple-overlay").click(() => {
                $("#register-modal").modal('show');
            });
        
    </script>

    


