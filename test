<script>
  $(".fb-login-button").data('width', '300').css({'border-radius': '0.8rem', 'font-size': '1.6rem'})

  function loginFacebookUser(response){
    // Log UserEvent record for SSO Facebook Click
    $.ajax({
      url: '/questions/userevent/',
      method: 'POST',
      data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "SFC"}
      }).done(function(response) {
      });
    FB.api('/me', {locale: 'en_US', fields: 'id,first_name,last_name,email,link,gender,locale,picture'}, function(res){
      let token = {
        'csrfmiddlewaretoken': 'o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf'
      }
      addSpinner();
      $.ajax({
        url: '/oauth/facebook/',
        method: 'POST',
        data: {...token, ...response.authResponse, ...res, renderedResponseNeeded: true},
        success: function(response){
          if (response.status == "Success") {
            
              window.location.reload();
            
          } else {
          removeSpinner();
          
            
              $('#subscription-modal-response-container').html(response);
              $('#register-modal').modal('hide');
              $('#qd-login-modal').modal('hide');
              $('#subscribe-modal').modal('show');
            
          
          }
        },
        error: function(res){
          removeSpinner();
          FB.login(function(response) {}, { auth_type: 'reauthenticate' });
          if(res.responseJSON.message == 'No Email'){
            errMessageHtml = `<p style="margin-top: 1rem;" class="flash-message error">Your email is required to login/sign up with Facebook. Please adjust your Facebook permissions to ensure your email is included.</p>`
            $(errMessageHtml).insertAfter('.fb-login-button')
          }
        }
      })
    });
  }
  $.ajaxSetup({ cache: true });
  $.getScript('https://connect.facebook.net/en_US/sdk.js', function(){
  FB.init({
    appId: '402458547807517',
    version: 'v2.7'
  });
  FB.getLoginStatus(function(response){
    if(response.status == 'connected'){
      loginFacebookUser(response);
    } else {
      $('#loginbutton,#feedbutton').removeAttr('disabled');
      FB.Event.subscribe('auth.login', function(response){
        loginFacebookUser(response);
      });
      FB.Event.subscribe('auth.logout', function(response){
        console.log(response);
      });
    }
  });

  });
</script>
