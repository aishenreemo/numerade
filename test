    <script>
        $('.banner-buttom').click(function(){
            $.ajax({
                    url: '/questions/userevent/',
                    method: 'POST',
                    data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "BSA"}
                    }).done(function(response) {
                });
            let email = $('.banner-email').val();
            if(email !== ''){
                $.ajax({
                url: '/send-email-app/',
                method: 'post',
                data: {
                    'email': email,
                    'csrfmiddlewaretoken': 'o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf'
                }
            }).done(function(response) {
                $('.banner-email').hide();
                $('.button-container').hide();
                $('.banner-success-box').css('display', 'inline-flex');
                $('.banner-text-success').text(email);
            });
            }                            
        });

        $('.banner-text').click(function(){
            $.ajax({
                    url: '/questions/userevent/',
                    method: 'POST',
                    data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "BLM"}
                    }).done(function(response) {
                });
        });

        $(document).ready(function () {
            // function showBannerPopup() {
            //     var x = window.matchMedia("(max-width: 820px)");
            //     if (!x.matches) {
            //         $('.top-popup-banner').slideToggle(250);
            //     }
            // };
            // setTimeout(showBannerPopup, 1500);
            $('.close-top-popup-banner').click(function () {
                $.ajax({
                    url: '/questions/userevent/',
                    method: 'POST',
                    data:{csrfmiddlewaretoken: "o5hKWpg8QrgVbgZdXr2BGBcO6PYKbxB1eeSKixW7qJZNBqQo8gYjsvDkNXyxiXqf", event: "BXX"}
                    }).done(function(response) {
                });

                $.ajax({
                    url: '/banner/email_no_logged/dismiss/',
                    method: 'GET',
                    success: function (res) {
                        $('.top-popup-banner').remove();
                    },
                    error: function (res) {
                        $('.top-popup-banner').remove();
                    }
                })
                $('.top-popup-banner').remove();
            });
        });

        $('.studyparty-email-form').on('submit', function(e){
            e.preventDefault();
        })
    </script>

    
      
