  <script>
  function addSpinner(messageText){
    if(messageText){
      $('body').append(`<div class="loading-spinner-container"><div class="quiz-loading-spinner"></div><div class="loading-spinner-text">${messageText}</div></div>`);
    } else {
      $('body').append(`<div class="loading-spinner-container"><div class="quiz-loading-spinner"></div></div>`);
    }
  }
  function removeSpinner(){
    $('.loading-spinner-container').remove();
  }
</script>

  
<script>
  var videoTitle = 'i-a-tiger-leaps-horizontally-from-a-75-m-high-rock-with-a-speed-of-30-ms-how-far-from-the-base-of-the-rock-will-she-land-2';
</script>
