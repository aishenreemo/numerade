<script>
    
      var userIsAuthenticated = false;
    
    var planDict = { 
      "monthly": "",
      "annual": ""
    };
    var planType = '';
    var newToken = "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH";
    if (typeof planId == 'undefined') {
      var planId = planDict['annual'];
      planType = 'annual';
    }
    
    var successRedirect = false;
    var promoRedirect = false;
    var ampRedirect = false;
    //var paypalLayout = "horizontal";
    var paypalLayout = 'vertical';

    

    

    

    

    function initializePaypal(){
      $('#paypal-button-container').html('');

      paypal.Buttons({
        style: {
          shape: 'pill',
          color: 'white',
          layout: 'horizontal',
          label: 'subscribe'
        },

        onInit: function(data, actions) {
          $('#terms-check').on('change', function(){
            // Style disabled state
            // Prevent submission on enter
            preventSubmission = !$('#terms-check').is(":checked");
            if (!preventSubmission) {
              actions.enable();
            } else {
              actions.disable();
            }
          })

        },
        
        createSubscription: function(data, actions) {
          $.ajax({
            url: '/payments/paypal/check-duplicate/',
            method: 'POST', 
            data: { 
              csrfmiddlewaretoken: newToken,
            },
            error: function(response){
              location.href = '/home/';
            }
          })

          $.ajax({
            url: '/questions/userevent/',
            method: 'POST',
            data:{csrfmiddlewaretoken: "a07NlZeRXCYX3PqYltpVvdYBhOyeYZgMQP98hM8jU98YBlj90isGroqYFFXxKYHH", event: "UPA"}
            }).done(function(response) {
          });

          return actions.subscription.create({
                'plan_id': planId
              });
        },
        onApprove: function(data, actions) {
          // Assume CSRF in form has been updated and existing var is old
          if (window.location.pathname === "/welcome/") {
            let paymentFormToken = $('#payment-form').find("input[name='csrfmiddlewaretoken']").val();
            newToken = paymentFormToken;
          }
          $.ajax({
            url: '/payments/paypal/',
            method: 'POST', 
            data: { 
              csrfmiddlewaretoken: newToken,
              paypal_subscription_id: data.subscriptionID,
              plan_type: $('#plan-select-option').val(),
              formLoc: window.location.pathname
            },
            success: function(response){

              // GTM Event
              dataLayerEvent = {'event': 'async-payment-submission'};
              try {
                let transactionData = {
                  'transactionId': response.internal_transaction_id,
                  'transactionTotal': response.amount,
                  'transactionAffiliation': "paypal",
                  'transactionProducts': [planId]
                }
                dataLayer.push(transactionData);
                dataLayerEvent.subscriptionPrice = response.amount;

                let productData = {
                  'name': planId,
                  'sku': planId,
                  'price': response.amount,
                  'quantity': 1
                }
                dataLayer.push(productData);

              } catch (error) {
                console.error(error);
              }

              dataLayer.push(dataLayerEvent);
              
              $('#subscribe-modal-two').modal('hide');
              $('#sub-confirmation-modal').modal('show');
              $('#subscribe-modal-3_5').on('hidden.bs.modal', function (e) {
                addSpinner();
                window.location.reload();
              });

              if(successRedirect){
                if(window.location.pathname == '/payments/'){
                  console.log('Current path is payments')
                  window.location.href = "/payments/success/";
                }else{
                  $(".confirmation-modal-continue").attr("href", "/payments/success/");
                }
              } else if(promoRedirect){
                if(window.location.pathname == '/payments/'){
                  window.location.href = "/study-mascot/confirmation/";
                }else{
                  $(".confirmation-modal-continue").attr("href", "/study-mascot/confirmation/");
                }
              } else if(ampRedirect){
                if(window.location.pathname == '/payments/'){
                  window.location.href = "#success=true";
                }else{
                  $(".confirmation-modal-continue").attr("href", "#success=true");
                }
              }else{
                if(window.location.pathname == '/payments/'){
                  window.location.href = '/home/';
                }else{
                  let currUrl = window.location.href;
                  $(".confirmation-modal-continue").attr("href", currUrl);
                }
              }
              
            },
            error: function(response){
              console.log(response);
            }
            })
          },
      }).render('#paypal-button-container');
    }

  if(paypal == undefined){
    jQuery.loadScript = function (url) {
      jQuery.ajax({
          url: "https://www.paypal.com/sdk/js?client-id=AbzzTDENC1IUFQPKfGjS52yWyr_eLPdyD_w_9tMIx7foVfPOf4FnRHquqZE7PcZwxSnLQaxHIKU6JtsU&vault=true&disable-funding=credit&intent=subscription",
          dataType: 'script',
          success: ()=>initializePaypal(),
          async: true
      });
  }
  }else{
    initializePaypal();
  }

</script>


